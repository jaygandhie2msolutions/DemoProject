<div class="container">
  <div class="sixteen columns">
    <div class="center logo"><!--four columns logo-->
      <a href="{{ shop.url }}" title="{{ langify_shop_name }}">
        {% if settings.use_logo %}
        <img src="{% comment %}ly_asset_replace_for_[ 'logo.png' | asset_url ]_begin{% endcomment %}{% assign ly_asset = 'logo-new.png' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  }}{% comment %}ly_asset_replace_end{% endcomment %}" alt="{{ langify_shop_name }}" data-src="{% comment %}ly_asset_replace_for_[ 'logo.png' | asset_url ]_begin{% endcomment %}{% assign ly_asset = 'logo-new.png' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  }}{% comment %}ly_asset_replace_end{% endcomment %}" {% if settings.use_logo_home %}data-src-home="{% comment %}ly_asset_replace_for_[ 'logo_home.png' | asset_url ]_begin{% endcomment %}{% assign ly_asset = 'logo_home.png' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  }}{% comment %}ly_asset_replace_end{% endcomment %}"{% endif %} />
        {% else %}
        {{ langify_shop_name }}
        {% endif %}
      </a>
    </div>
  </div>

  <div class="sixteen columns center">
    <h1 class=" collection_title">Select your location</h1>
  </div>
</div>



<div class="container hidden-xs">
