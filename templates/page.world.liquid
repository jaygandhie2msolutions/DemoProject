<div class="panel-group visible-xs" id="accordion">
  <!-- first panel -->
  <div class="panel panel-default">
    <div class="panel-heading">
      <h4 class="panel-title">
        <a class="accordion-toggle collapsed"  data-toggle="collapse" data-parent="#accordion" href="#north-america-mobile">
         <span> {{ 'north america' | upcase }} </span>
       </a>
     </h4>
   </div>
   <div id="north-america-mobile" class="panel-collapse collapsing">
    <div class="panel-body">

      <div class="country-wrap america-countries-wrap">
        <ul class="america-countries nav nav-pills text-left">
          {% assign half = 21 %}
          {% assign rightcol = "" %}{% assign leftcol = "" %}
          {% for link in linklists[settings.popover_links_americas].links %}
          {% assign: extraClass = "" %}
          {% if forloop.index <= half %}
          {% if rightcol == "" %}
          {% assign: extraClass = "top-item" %}
          {% endif %}
          {% capture rightcol %}{% if rightcol != "" %}{{ rightcol }}|||{% endif %}
          <li{% include 'country-lang-switcher' with link.title %} class="ly-languages-switcher-link current_lang country-list-item {{ extraClass }} col-xs-12" data-value="{% comment %}ly_code_replace_for_[ link.title ]_begin{% endcomment %}{% include 'ly-title' with link %}{{ ly_translation }}{% comment %}ly_code_replace_end{% endcomment %}"><span class="flag-icon flag-icon-{% capture fl %}{% include 'countries' with link.title %}{% endcapture %}{{ fl | strip }}"></span>{% comment %}ly_code_replace_for_[ link.title ]_begin{% endcomment %}{% include 'ly-title' with link %}{{ ly_translation }}{% comment %}ly_code_replace_end{% endcomment %}</li>
          {% endcapture %}
          {% else %}
          {% if leftcol == "" %}
          {% assign: extraClass = "top-item" %}
          {% endif %}
          {% capture leftcol %}{% if leftcol != "" %}{{ leftcol }}|||{% endif %}
          <li{% include 'country-lang-switcher' with link.title %} class="ly-languages-switcher-link current_lang country-list-item {{ extraClass }} col-xs-12" data-value="{% comment %}ly_code_replace_for_[ link.title ]_begin{% endcomment %}{% include 'ly-title' with link %}{{ ly_translation }}{% comment %}ly_code_replace_end{% endcomment %}"><span class="flag-icon flag-icon-{% capture fl %}{% include 'countries' with link.title %}{% endcapture %}{{ fl | strip }}"></span>{% comment %}ly_code_replace_for_[ link.title ]_begin{% endcomment %}{% include 'ly-title' with link %}{{ ly_translation }}{% comment %}ly_code_replace_end{% endcomment %}</li>
          {% endcapture %}
          {% endif %}

          {% endfor %}

          {% assign leftcol = leftcol | split: "|||" %}
          {% assign rightcol = rightcol | split: "|||" %}

          {% for link in rightcol %}
          {{ link }}
          {% if leftcol[forloop.index0] %}{{ leftcol[forloop.index0] }}
          {% else %}<li class="country-list-item country-list-item-empty{% if forloop.index0 == 0 %} top-item{% endif %}"></li>
          {% endif %}
          {% endfor %}
        </ul>
      </div>

    </div><!-- body -->
  </div><!-- north-america -->
</div><!-- panel panel-default -->
